{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "string"
    },
    "Content": {
      "type": "richtext"
    },
    "User": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    },
    "Pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Date": {
      "type": "date"
    },
    "Category": {
      "type": "string"
    },
    "InitialExibition": {
      "type": "boolean"
    }
  }
}
